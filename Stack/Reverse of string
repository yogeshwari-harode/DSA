#include<bits/stdc++.h>
using namespace std;

void ReverseOfString(string s)
{
    stack<char>S;
    for(int i=0 ; i<s.size() ; i++)
    {
        S.push(s[i]);
    }
    
    for(int i=0 ; i<s.size() ; i++)
    {
        cout<<S.top();
        S.pop();
    }
}

int main()
{
    string s;
    cin>>s;
    ReverseOfString(s);
    return 0;
}
